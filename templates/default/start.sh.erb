#!/bin/bash
set -e

SERVICE_NAME=delabootstrap
BASE_DIR=<%= node.delabootstrap.base_dir %>
PID_FILE=<%= node.delabootstrap.pid_file %>
LOGS=<%= node.delabootstrap.logs %>
CMD=${BASE_DIR}/bin/generic_start.sh  

rm -f <%= node.delabootstrap.pid_file %>
rm -f ${LOGS}
$CMD $SERVICE_NAME $BASE_DIR $PID_FILE $LOGS